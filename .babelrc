{
  "presets": ["latest", "react"],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true
    }]
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"],
      "plugins": ["transform-react-jsx-source", "transform-react-jsx-self"]
    },
    "production": {
      "plugins": ["transform-react-constant-elements"]
    }
  }
}
