{
  "name": "react-starter-kit",
  "version": "0.0.0",
  "author": "Xiaofan Wu (https://github.com/xiaofan2406)",
  "description": "A simple boilerplate for starting new React projects.",
  "keywords": [
    "react",
    "react-router",
    "redux",
    "mobx",
    "emotion",
    "flow",
    "boilerplate"
  ],
  "repository": "xiaofan2406/react-starter-kit",
  "bugs": {
    "url": "https://github.com/xiaofan2406/react-starter-kit/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint:eslint": "eslint src config",
    "lint:prettier": "prettier-check '{src,config}/**/*.{js,mjs,json,css,md}'",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "format": "prettier '{src,config}/**/*.{js,mjs,json,css,md}' --write",
    "dev":
      "NODE_ENV=development webpack-dev-server --config ./config/webpack.dev.js",
    "test": "NODE_ENV=test jest --watch --updateSnapshot",
    "coverage": "NODE_ENV=test jest --coverage",
    "prebuild": "yarn lint && yarn flow && rm -rf ./dist",
    "build": "NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "start": "node ./config/serve.js"
  },
  "lint-staged": {
    "*.{js,mjs,json,css,md}": ["prettier --write", "git add"]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "preset": "<rootDir>/config/jest/jest.config.json"
  },
  "browserslist": [
    "Chrome >= 60",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Firefox >= 54",
    "Edge >= 15"
  ],
  "dependencies": {
    "emotion": "^9.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-emotion": "^9.1.2",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-flow": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-emotion": "^9.1.2",
    "babel-plugin-transform-dynamic-import": "^2.0.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "filemanager-webpack-plugin": "^1.0.26",
    "flow-bin": "^0.70.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-emotion": "^9.1.2",
    "json-loader": "^0.5.7",
    "lint-staged": "^7.0.4",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "prettier": "^1.12.1",
    "prettier-check": "^2.0.0",
    "react-hot-loader": "^4.1.2",
    "react-test-renderer": "^16.3.2",
    "serve": "^6.5.6",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  }
}
