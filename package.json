{
  "name": "react-starter-kit",
  "version": "0.0.0",
  "author": "Xiaofan Wu (https://github.com/xiaofan2406)",
  "description": "A simple boilerplate for starting new React projects.",
  "keywords": [
    "react",
    "@reach/router",
    "boilerplate"
  ],
  "repository": "xiaofan2406/react-starter-kit",
  "bugs": {
    "url": "https://github.com/xiaofan2406/react-starter-kit/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint:eslint": "eslint '**/*.{js,mjs}' --ignore-path .gitignore",
    "lint:prettier": "prettier-check '**/*.{js,mjs,json,css,md}' --ignore-path .gitignore",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "format": "prettier '**/*.{js,mjs,json,css,md}' --write --ignore-path .gitignore",
    "dev": "NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js",
    "test": "NODE_ENV=test jest --watch --updateSnapshot",
    "coverage": "NODE_ENV=test jest --coverage",
    "prebuild": "yarn lint && rm -rf ./dist",
    "build": "NODE_ENV=production webpack --config ./config/webpack.config.js",
    "start": "serve ./dist -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,mjs,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "browserslist": [
    "Chrome >= 60",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Firefox >= 54",
    "Edge >= 15"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@reach/router": "^1.2.1",
    "core-js": "^3.1.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@emotion/babel-preset-css-prop": "^10.0.14",
    "@testing-library/react": "^8.0.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "css-loader": "^3.1.0",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.1",
    "ip": "^1.1.5",
    "is-wsl": "^2.1.0",
    "jest": "^24.8.0",
    "jest-emotion": "^10.0.14",
    "json-loader": "^0.5.7",
    "lint-staged": "^9.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.18.2",
    "prettier-check": "^2.0.0",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.8.6",
    "serve": "^11.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
