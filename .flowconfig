[ignore]
.*/config/.*
.*/coverage/.*
.*/dist/.*
.*/node_modules/.*
.*/__snapshots__/.*

[include]

[libs]
./src/store/types.js

[lints]

[options]
module.file_ext=.js
module.file_ext=.mjs
module.file_ext=.json
module.use_strict=true
experimental.const_params=true
module.name_mapper='^assets' -> '<PROJECT_ROOT>/src/assets'
module.name_mapper='^components' -> '<PROJECT_ROOT>/src/components'
module.name_mapper='^factories' -> '<PROJECT_ROOT>/src/factories'
module.name_mapper='^store' -> '<PROJECT_ROOT>/src/store'
module.name_mapper='^styles' -> '<PROJECT_ROOT>/src/styles'
module.name_mapper='^utils' -> '<PROJECT_ROOT>/src/utils'
module.name_mapper='^widgets' -> '<PROJECT_ROOT>/src/widgets'
